<vector xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:aapt="http://schemas.android.com/aapt"
	android:width="200dp"
	android:height="200dp"
	android:viewportWidth="200"
	android:viewportHeight="200">
	<path
		android:pathData="M100,100m-96,0a96,96 0,1 1,192 0a96,96 0,1 1,-192 0"
		android:strokeWidth="4">
		<aapt:attr name="android:strokeColor">
			<gradient
				android:type="linear"
				android:angle="45"
				android:startX="20"
				android:startY="20"
				android:endX="180"
				android:endY="180">
				<item android:color="#FF00FF" android:offset="0"/>
				<item android:color="#00FFFF" android:offset="0.5"/>
				<item android:color="#FF00FF" android:offset="1"/>
			</gradient>
		</aapt:attr>
	</path>
	<path
		android:pathData="M100,100m-90,0a90,90 0,1 1,180 0a90,90 0,1 1,-180 0"
		android:strokeWidth="3">
		<aapt:attr name="android:strokeColor">
			<gradient
				android:type="linear"
				android:angle="135"
				android:startX="50"
				android:startY="50"
				android:endX="150"
				android:endY="150">
				<item android:color="#90CAF9" android:offset="0" android:alpha="0.3"/>
				<item android:color="#00DDFF" android:offset="0.5" android:alpha="0.3"/>
				<item android:color="#90CAF9" android:offset="1" android:alpha="0.3"/>
			</gradient>
		</aapt:attr>
	</path>
</vector>